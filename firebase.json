{
  "hosting": {
    "public": "frontend",
    "rewrites": [
      {
        "source": "/jay-status",
        "function": "jayStatusApp"
      },
      {
        "source": "/jay-ino",
        "function": "jayStatusIno"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ]
}
